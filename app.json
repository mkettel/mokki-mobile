{
  "expo": {
    "name": "mokki-mobile",
    "slug": "mokki-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mokkimobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "fonts": [
      "./assets/fonts/Chillax-Regular.ttf",
      "./assets/fonts/Chillax-Medium.ttf",
      "./assets/fonts/Chillax-Semibold.ttf",
      "./assets/fonts/Chillax-Bold.ttf",
      "./assets/fonts/Boska-Regular.ttf",
      "./assets/fonts/Boska-Medium.ttf",
      "./assets/fonts/Boska-Bold.ttf"
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
