{
  "expo": {
    "name": "MÃ¶kki",
    "slug": "mokki-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mokki",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "fonts": [
      "./assets/fonts/Chillax-Regular.otf",
      "./assets/fonts/Chillax-Medium.otf",
      "./assets/fonts/Chillax-Semibold.otf",
      "./assets/fonts/Chillax-Bold.otf",
      "./assets/fonts/Boska-Regular.otf",
      "./assets/fonts/Boska-Medium.otf",
      "./assets/fonts/Boska-Bold.otf"
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#D9CEBC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.mokki.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSCameraUsageDescription": "Mokki uses your camera to take photos and videos for group chat, capture media for the shared gallery, and photograph receipts for expense tracking.",
        "NSMicrophoneUsageDescription": "Mokki uses your microphone to record audio when capturing videos for group chat and the shared media gallery.",
        "NSPhotoLibraryUsageDescription": "Mokki uses your photo library to set your profile picture, share photos and videos in group chat, upload media to the shared gallery, and attach receipt photos to expenses."
      },
      "associatedDomains": [
        "applinks:mokkiski.com",
        "applinks:www.mokkiski.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#D9CEBC"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Chillax-Regular.otf",
            "./assets/fonts/Chillax-Medium.otf",
            "./assets/fonts/Chillax-Semibold.otf",
            "./assets/fonts/Chillax-Bold.otf",
            "./assets/fonts/Boska-Regular.otf",
            "./assets/fonts/Boska-Medium.otf",
            "./assets/fonts/Boska-Bold.otf"
          ]
        }
      ],
      "expo-notifications",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b861798e-a373-438c-98f8-d8d181d16ef4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b861798e-a373-438c-98f8-d8d181d16ef4"
    }
  }
}
